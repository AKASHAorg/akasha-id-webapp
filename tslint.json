{
    "extends": [
      "tslint-config-airbnb",
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier",
      "tslint-plugin-prettier"
    ],
    "rules": {
      "import-name": [false],
      "jsx-no-lambda": false,
      "prettier": true,
      "interface-name": false,
      "object-literal-sort-keys": false,
      "no-empty": false
    },
    "linterOptions": {
      "exclude": [
        "src/serviceWorker.ts"
      ]
    }
  }