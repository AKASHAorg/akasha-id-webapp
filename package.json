{
  "name": "masq-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts start",
    "build": "REACT_APP_GIT_SHA=`git rev-parse --short HEAD` react-scripts build",
    "test": "concurrently -k -s first \"signalhubws -p 8080\" \"karma start karma.conf.js\"",
    "lint": "eslint --fix --ext .js,.jsx ./src",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "bluebird": "^3.5.3",
    "classnames": "^2.2.6",
    "compressorjs": "^1.0.5",
    "detect-browser": "^3.0.1",
    "masq-common": "git+https://github.com/QwantResearch/masq-common",
    "pump": "^3.0.0",
    "qwant-research-components": "git+https://github.com/QwantResearch/qwant-research-components.git",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^6.0.0",
    "react-responsive": "^6.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "signalhubws": "^1.0.7",
    "typeface-asap": "0.0.54",
    "uuid": "^3.3.2",
    "webrtc-swarm": "git+https://github.com/QwantResearch/webrtc-swarm#masq"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "es6-promisify": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "hyperdb": "^3.5.0",
    "karma": "^4.0.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.0.2",
    "node-sass": "^4.9.4",
    "puppeteer": "^1.13.0",
    "qrcode.react": "^0.9.3",
    "random-access-memory": "^3.1.1",
    "webpack-cli": "^3.2.3"
  }
